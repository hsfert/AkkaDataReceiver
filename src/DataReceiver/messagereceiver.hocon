akka {
	actor {
		provider = cluster
		deployment {
			 
		}
	}
	remote {				
		dot-netty.tcp {
			hostname = "localhost"
			port = 0
		}
	}            

	cluster {
		seed-nodes = ["akka.tcp://datareceiver@localhost:4056"] 
		roles = [messagereceiver]
		downing-provider-class = "Akka.Cluster.SBR.SplitBrainResolverProvider, Akka.Cluster"
		split-brain-resolver {
			active-strategy = static-quorum
			static-quorum {
				quorum-size = 1
				role = "lighthouse"
			}
		}
	}
}